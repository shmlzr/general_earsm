@inproceedings{Escobar2013a,
abstract = {When the Navier-Stokes equations are applied into turbulence, closure equations are necessary in addition to an average approach of the associated variables. These equations relate the Reynolds stress tensor with kinematic tensors. The methodology here presented quantifies the dependence of the Reynolds tensor with kinematic tensors such as the rate-of-strain and the non-persistence-of-strain tensors. The methodology is based on tensor decomposition theorems from the Reynolds stress tensor (anisotropic) obtained from LES are extracted parts in phase and parts out of phase with each kinematic tensor. The proposed RANS basis are then compared with the Reynolds tensor which is obtained by a LES in a backward facing step. Local measurers of the accuracy of each model are employed, becoming possible to quantify their adequacy as a function of the position.The tensor basis used in this study are capable of representing more faithfully the Reynolds stress tensor then the methods used by industry today, and require low computational cost.},
author = {Escobar, Pascoal and Alves, Fernando Soares and Eduardo, Luiz and Sampaio, Bittencourt and David, Raphael and Bacchi, Aquilino and Thompson, Roney Leon},
booktitle = {22nd International Congress of Mechanical Engineering (COBEM 2013)},
file = {:Users/mschmelzer/Documents/PAPER/2013 - Escobar et al. - R Model formulations from les.pdf:pdf},
keywords = {les,persistence of straining,tensor decomposition,turbulence modeling},
number = {Cobem},
