@article{Duraisamy2019,
abstract = {Data from experiments and direct simulations of turbulence have historically been used to calibrate simple engineering models such as those based on the Reynolds-averaged Navier–Stokes (RANS) equations. In the past few years, with the availability of large and diverse data sets, researchers have begun to explore methods to systematically inform turbulence models with data, with the goal of quantifying and reducing model uncertainties. This review surveys recent developments in bounding uncertainties in RANS models via physical constraints, in adopting statistical inference to characterize model coefficients and estimate discrepancy, and in using machine learning to improve turbulence models. Key principles, achievements, and challenges are discussed. A central perspective advocated in this review is that by exploiting foundational knowledge in turbulence modeling and physical constraints, researchers can use data-driven approaches to yield useful predictive models.},
author = {Duraisamy, Karthik and Iaccarino, Gianluca and Xiao, Heng},
doi = {10.1146/annurev-fluid-010518-040547},
file = {:Users/mschmelzer/Documents/PAPER/2019 - Duraisamy, Iaccarino, Xiao - Turbulence Modeling in the Age of Data - Annual Review of Fluid Mechanics.pdf:pdf},
journal = {Annual Review of Fluid Mechanics},
number = {1},
title = {{Turbulence Modeling in the Age of Data}},
volume = {51},
year = {2019}
}
@article{Holzmann2016a,
abstract = {The content of this book is the derivation of all equations in a full and clear way. Therefore, we derive the mass, momentum, total energy, mechanical (kinetic) energy, thermo (internal) energy, enthalpy, temperature and the generalized conservation equations. Furthermore, different kinds of notations are given like the conserved, non-conserved and integral form. After the derivation of all equations, a summary is given and we derive the necessary equations for incompressible turbulent flow fields using the Reynolds time-averaging concept. Here, we will derive the incompressible mass, momentum, Reynolds-Stress, kinetic energy of the turbulence and dissipation equations and show the relations between them. At the end we demonstrate how to derive the equations for compressible flow fields by introducing the Favre averaging concept and applying this to the compressible mass conservation equation. Finally we will analyze the calculation of the shear-rate tensor in OpenFOAM{\textregistered}. This book give the basic knowledge for further model development especially due to the fact that we demonstrate the relations between the Cauchy-Stress tensor, shear-rate tensor and the pressure (also we will show analogies with the Reynolds-Stress tensor). Mathematics, Numerics, Derivations and OpenFOAM{\textregistered}. Available from: https://www.researchgate.net/publication/303793079{\_}Mathematics{\_}Numerics{\_}Derivations{\_}and{\_}OpenFOAMR [accessed Jul 22, 2016].},
