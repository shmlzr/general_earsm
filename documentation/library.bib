doi = {10.1109/TPWRS.2013.2287871},
file = {:Users/mschmelzer/Documents/PAPER/2014 - Wan et al. - Probabilistic forecasting of wind power generation using extreme learning machine - IEEE Transactions on Power Syste.pdf:pdf},
