keywords = {Algebraic flux models,RANS,Reynolds stress anisotropy,Scalar dispersion,V2-f,Wavy wall},
number = {5},
pages = {943--963},
publisher = {Elsevier Inc.},
title = {{The influence of normal stress anisotropy in predicting scalar dispersion with the v2-f model}},
url = {http://dx.doi.org/10.1016/j.ijheatfluidflow.2011.06.010},
volume = {32},
year = {2011}
