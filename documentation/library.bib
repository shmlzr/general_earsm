@article{Hamlington2008,
abstract = {From consideration of turbulence anisotropy dynamics due to spatial$\backslash$nor temporal variations in the mean strain rate, a new Reynolds stress$\backslash$nclosure for nonequilibrium effects in turbulent flows has been developed.$\backslash$nThis closure, formally derived from the Reynolds stress anisotropy$\backslash$ntransport equation, results in an effective strain rate tensor that$\backslash$naccounts for the strain rate history to which the turbulence has$\backslash$nbeen subjected. In contrast to prior nonequilibrium models that have$\backslash$nsought to address nonequilibrium effects via changes in the eddy$\backslash$nviscosity, the present approach accounts for nonequilibrium effects$\backslash$nin the fundamental relation between the anisotropy tensor and the$\backslash$nstrain rate tensor. The time-local form of the nonequilibrium closure$\backslash$ncan be readily implemented in place of the classical equilibrium$\backslash$nBoussinesq closure on which most existing computational frameworks$\backslash$nare currently based. This new closure is applied here to four substantially$\backslash$ndifferent classes of nonequilibrium test problems. Results show dramatically$\backslash$nimproved agreement with experimental and computational data, without$\backslash$nthe need to vary any model parameters, when compared with the standard$\backslash$nequilibrium closure and with various prior nonequilibrium closures.$\backslash$n(c) 2008 American Institute of Physics.},
author = {Hamlington, Peter E. and Dahm, Werner J.},
doi = {10.1063/1.3006023},
file = {:Users/mschmelzer/Documents/PAPER/2008 - Hamlington, Dahm - Reynolds stress closure for nonequilibrium effects in turbulent flows - Physics of Fluids.pdf:pdf},
