file = {:Users/mschmelzer/Documents/PAPER/2018 - Wu, Xiao, Paterson - Physics-Informed Machine Learning Approach for Augmenting Turbulence Models A Comprehensive Framework - Unkn.pdf:pdf},
issn = {2469-990X},
keywords = {RANS equations,Reynolds stress,integrity basis,machine learning,random forest,turbulence modeling},
number = {July},
title = {{Physics-Informed Machine Learning Approach for Augmenting Turbulence Models: A Comprehensive Framework}},
url = {https://arxiv.org/pdf/1801.02762.pdf},
year = {2018}
}
