keywords = {Finite volume method,Hybrid turbulence model,Low-Re number model,Reynolds-averaged Navier-Stokes equations},
number = {1},
pages = {89--116},
title = {{A new hybrid turbulence modelling strategy for industrial CFD}},
volume = {42},
year = {2003}
}
