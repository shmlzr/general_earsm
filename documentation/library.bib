isbn = {978-1-4471-1101-6},
journal = {Soft Computing and Industry},
number = {1992},
pages = {635--653},
title = {{Gene Expression Programming in Problem Solving}},
url = {http://link.springer.com/10.1007/978-1-4471-0123-9{\_}54},
year = {2002}
}
