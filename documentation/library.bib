keywords = {Bayesian Model-Scenario Averaging,Bayesian model averaging,Boundary-layers,Calibration,Error estimation,Model inadequacy,RANS models,Uncertainty quantification},
pages = {65--91},
publisher = {Elsevier Inc.},
title = {{Predictive RANS simulations via bayesian model-scenario averaging}},
