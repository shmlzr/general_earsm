title = {{A New Physically-Based Fully-Realizable Nonequilibrium Reynolds Stress Closure for Turbulent Flow RANS Modeling.}},
url = {http://www.aric.or.kr/treatise/journal/content.asp?idx=115088},
volume = {2007-5573},
